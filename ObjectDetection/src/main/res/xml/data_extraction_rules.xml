<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude sensitive data from cloud backup -->
        <exclude domain="cache" />
        <exclude domain="external" path="." />
    </cloud-backup>
    <device-transfer>
        <!-- Allow device transfer for app data -->
        <include domain="file" path="." />
    </device-transfer>
</data-extraction-rules>